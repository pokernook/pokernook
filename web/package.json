    "@types/node": "^17.0.42",
    "@types/react": "^17.0.5",
    "@types/react-dom": "^17.0.2",
    "@types/seedrandom": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "concurrently": "^6.0.2",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.3.0",
