    "@urql/devtools": "^2.0.3",
    "@urql/exchange-graphcache": "^4.1.0",
    "boardgame.io": "^0.44.2",
    "date-fns": "^2.21.3",
    "emoji-mart": "^3.0.1",
    "framer-motion": "^4.1.11",
    "graphql": "^15.5.0",
    "next": "^10.2.0",
    "next-transpile-modules": "^7.0.0",
    "next-urql": "^3.0.0",
    "path-to-regexp": "^6.2.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-easy-crop": "^3.3.3",
